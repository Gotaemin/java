package service;

import static db.JdbcUtil.getConnection;

import java.sql.Connection;

import dao.BbsDAO;
import vo.Bbs;

public class BbsViewService {

	public Bbs getBbs(int bbsID) {
		Connection conn = getConnection();
		BbsDAO bbsDAO = new BbsDAO();
		bbsDAO.setConnection(conn);
		
		bbsDAO.getBbs(bbsID);
		
		return null;
	}

}
